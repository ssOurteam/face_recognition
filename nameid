# face_recognition
class Face_id:
    def __init__(self,name,rel,id):
        self.name=name
        self.rel=rel
        self.id=id
    def get_name(self):
        return self.name
    def get_rel(self):
        return self.rel
    def get_id(self):
        return self.id

test_list=[]

face1=Face_id('jin','brother1',1)
face2=Face_id('awei','brother2',2)
face3=Face_id('yzj','son',3)
face4=Face_id('pyy','py',4)
test_list=[face1,face2,face3,face4]


        
        
